  "editor.fontFamily": "Fira Code Retina, Monaco, 'Courier New', monospacenew",
  "editor.fontSize": 15,
  "terminal.integrated.fontFamily": "monospace",
  "go.formatTool": "goimports",
  "erlang_ls.serverPath": "/Users/codeadict/erlang_ls/_build/default/bin/erlang_ls"
