    "*.eex": "HTML (EEx)",
    "*.leex": "HTML (EEx)",
    "*.html.EEx": "HTML (EEx)"
  },
  "beautify.language": {
    "js": [],
    "css": [],
    "html": ["HTML (EEx)"]
  },
